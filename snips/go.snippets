snippet iferr "Error check" b
if err != nil {
	${0:return err}
}
endsnippet

snippet struct "Struct declaration" b
type ${1:sneed} struct {
	$0
}
endsnippet

snippet int "Interface declaration" b
type ${1:sneed} interface {
	$0
}
endsnippet

snippet func "Function declaration" b
func ${1:sneed}($2) $3`p snip.rv = ' ' if t[3] != '' else ''`{
	$0
}
endsnippet

snippet meth "Method declaration" b
func (${1:s} ${2:*sneed}) ${3:feed}($4) $5`p snip.rv = ' ' if t[3] != '' else ''`{
	$0
}
endsnippet
